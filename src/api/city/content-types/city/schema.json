{
  "kind": "collectionType",
  "collectionName": "cities",
  "info": {
    "singularName": "city",
    "pluralName": "cities",
    "displayName": "City",
    "description": "City-specific data and SEO content"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "metaKeywords": {
      "type": "json"
    },
    "heroTitle": {
      "type": "string",
      "required": true
    },
    "heroSubtitle": {
      "type": "text"
    },
    "services": {
      "type": "json"
    },
    "stats": {
      "type": "json"
    },
    "reviews": {
      "type": "json"
    },
    "faqs": {
      "type": "json"
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "state": {
      "type": "string",
      "required": true
    },
    "country": {
      "type": "string",
      "default": "India"
    },
    "latitude": {
      "type": "float"
    },
    "longitude": {
      "type": "float"
    },
    "pincode": {
      "type": "string"
    },
    "serviceRadius": {
      "type": "integer",
      "default": 25
    },
    "isServiceAvailable": {
      "type": "boolean",
      "default": true
    },
    "priority": {
      "type": "integer",
      "default": 0
    },
    "metaDescription": {
      "type": "text"
    },
    "canonicalUrl": {
      "type": "string"
    },
    "ogImage": {
      "type": "string"
    }
  }
} 